<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/abu"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <androidx.cardview.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/img_item_photo"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:contentDescription="@string/foto_profil"
            android:scaleType="centerCrop"
            tools:ignore="MissingConstraints,ImageContrastCheck"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="25dp"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/tv_item_username"
            android:layout_width="200dp"
            android:layout_height="18dp"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="43dp"
            tools:text="@string/circle_name" />

        <TextView
            android:id="@+id/tv_item_caption"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:ellipsize="end"
            android:maxLines="3"
            android:textColor="#455A64"
            android:textSize="16sp"
            tools:ignore="MissingConstraints,TextContrastCheck"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="88dp"
            tools:text="@string/circle_caption" />

        <ImageView
            android:id="@+id/img_item_post"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:contentDescription="@string/foto_post"
            android:scaleType="centerCrop"
            android:src="@drawable/start2img"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="146dp" />

        <ImageView
            android:id="@+id/iconSuka"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/like"
            card_view:srcCompat="@drawable/ic_suka"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="352dp" />

        <ImageView
            android:id="@+id/iconKomentar"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/comment"
            card_view:srcCompat="@drawable/ic_komentar"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="115dp"
            tools:layout_editor_absoluteY="352dp" />

        <ImageView
            android:id="@+id/iconBagikan"
            android:layout_width="27dp"
            android:layout_height="21dp"
            android:contentDescription="@string/share"
            card_view:srcCompat="@drawable/ic_share"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="350dp"
            tools:layout_editor_absoluteY="356dp" />

        <TextView
            android:id="@+id/comment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="160dp"
            android:textSize="16sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="352dp"
            tools:text="@string/comment" />

        <TextView
            android:id="@+id/like"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="296dp"
            android:textSize="16sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="352dp"
            tools:text="@string/like" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="387dp">

        <ImageView
            android:id="@+id/img_item_photo2"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:contentDescription="@string/foto_profil"
            android:scaleType="centerCrop"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="25dp"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/tv_item_username2"
            android:layout_width="200dp"
            android:layout_height="18dp"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="43dp"
            tools:text="@string/circle_name" />

        <TextView
            android:id="@+id/tv_item_caption2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:ellipsize="end"
            android:maxLines="3"
            android:textColor="#795548"
            android:textSize="16sp"
            tools:ignore="MissingConstraints,TextContrastCheck"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="88dp"
            tools:text="@string/circle_caption" />

        <ImageView
            android:id="@+id/img_item_post2"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:contentDescription="@string/foto_post"
            android:scaleType="centerCrop"
            android:src="@drawable/start2img"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="146dp" />

        <ImageView
            android:id="@+id/iconSuka2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/like"
            card_view:srcCompat="@drawable/ic_suka"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="357dp" />

        <ImageView
            android:id="@+id/iconKomentar2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/comment"
            card_view:srcCompat="@id/ic_suka"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="114dp"
            tools:layout_editor_absoluteY="357dp" />

        <ImageView
            android:id="@+id/iconBagikan2"
            android:layout_width="27dp"
            android:layout_height="21dp"
            android:contentDescription="@string/share"
            card_view:srcCompat="@drawable/ic_share"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="341dp"
            tools:layout_editor_absoluteY="356dp" />

        <TextView
            android:id="@+id/comment2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="152dp"
            android:textSize="16sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints,TextContrastCheck"
            tools:layout_editor_absoluteY="356dp"
            tools:text="@string/comment" />

        <TextView
            android:id="@+id/like2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="292dp"
            android:textSize="16sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="356dp"
            tools:text="@string/like" />

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>

