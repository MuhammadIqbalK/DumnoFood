<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dumnofood.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.dumnofood.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_takeout"
        android:name="com.example.dumnofood.ui.takeout.TakeoutFragment"
        android:label="Takeout"
        tools:layout="@layout/fragment_takeout" />

    <fragment
        android:id="@+id/navigation_circle"
        android:name="com.example.dumnofood.ui.circle.CircleFragment"
        android:label="Circle"
        tools:layout="@layout/fragment_circle" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.dumnofood.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />



</navigation>